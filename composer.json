{
    "name": "bacart/webdav-client",
    "description": "Bacart WebDAV client",
    "type": "library",
    "keywords": [
        "php",
        "webdav",
        "bacart"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "bacart/guzzle-client": "^0.1",
        "symfony/dom-crawler": "^4.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "wa72/htmlpagedom": "^1.3"
    },
    "suggest": {
        "phpstan/phpstan": "PHPStan - PHP Static Analysis Tool",
        "wa72/htmlpagedom": "PHP implementation of the jQuery DOM manipulation API for HTML documents"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bacart\\WebdavClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
